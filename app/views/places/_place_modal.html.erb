<div class="md-modal md-effect-1" id="<%=place.title%>">
  <div class="md-content">
    <h3 class="tagline"><%= place.tagline.html_safe %></h3>
    <div>
      <p class="excerpt">
        <%= place.description %>
      </p>
      <div class="md-bottom">
        <p data-youtube="true" data-video-id="<%= place.youtube_link %>">Écouter la musique associée</p>
      </div>
    </div>
  </div>
</div>